{"ast":null,"code":"import Connexion from \"@/views/Connexion.vue\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    Connexion\n  },\n\n  data() {\n    let isConnected = false;\n    let isLoading = true;\n    return {\n      isConnected,\n      isLoading\n    };\n  },\n\n  methods: {\n    // Button déconnexion\n    deco() {\n      this.isConnected = false;\n    },\n\n    // Button connexion\n    connect() {\n      this.isConnected = true;\n      this.loading();\n    },\n\n    // Requette a l'api et page de chargement\n    async loading() {\n      this.isLoading = false;\n      this.$store.dispatch(\"set_donnees\", await axios.get(\"http://localhost:5400/search\").then(reponse => reponse.data).then(this.isLoading = true).catch(function (error) {\n        console.log(\" error : \", error);\n      }));\n      console.log(this.$store.getters.get_table);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuBA,OAAOA,SAAP,MAAsB,uBAAtB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVF;AADU,GADC;;AAIbG,MAAI,GAAG;AACL,QAAIC,WAAU,GAAI,KAAlB;AACA,QAAIC,SAAQ,GAAI,IAAhB;AAEA,WAAO;AACLD,iBADK;AAELC;AAFK,KAAP;AAID,GAZY;;AAabC,SAAO,EAAE;AACP;AACAC,QAAI,GAAG;AACL,WAAKH,WAAL,GAAmB,KAAnB;AACD,KAJM;;AAKP;AACAI,WAAO,GAAG;AACR,WAAKJ,WAAL,GAAmB,IAAnB;AACA,WAAKK,OAAL;AACD,KATM;;AAUP;AACA,UAAMA,OAAN,GAAgB;AACd,WAAKJ,SAAL,GAAiB,KAAjB;AACA,WAAKK,MAAL,CAAYC,QAAZ,CACE,aADF,EAEE,MAAMV,KAAI,CACPW,GADG,CACC,8BADD,EAEHC,IAFG,CAEGC,OAAD,IAAaA,OAAO,CAACX,IAFvB,EAGHU,IAHG,CAGG,KAAKR,SAAL,GAAiB,IAHpB,EAIHU,KAJG,CAIG,UAAUC,KAAV,EAAiB;AACtBC,eAAO,CAACC,GAAR,CAAY,WAAZ,EAAyBF,KAAzB;AACD,OANG,CAFR;AAUAC,aAAO,CAACC,GAAR,CAAY,KAAKR,MAAL,CAAYS,OAAZ,CAAoBC,SAAhC;AACD;;AAxBM;AAbI,CAAf","names":["Connexion","axios","components","data","isConnected","isLoading","methods","deco","connect","loading","$store","dispatch","get","then","reponse","catch","error","console","log","getters","get_table"],"sourceRoot":"","sources":["D:\\www\\tristan.labaume\\Année 2021 - 2022\\M. Paul\\projet_vu