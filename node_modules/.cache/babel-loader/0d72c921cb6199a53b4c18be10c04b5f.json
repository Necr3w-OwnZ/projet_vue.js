{"ast":null,"code":"import Connexion from \"@/views/Connexion.vue\";\nexport default {\n  components: {\n    Connexion\n  },\n\n  data() {\n    let isConected = true;\n    return {\n      isConected\n    };\n  },\n\n  methods: {\n    // Faire des requettes a l'api\n    request_api: async function () {\n      const result = await axios.get(\"http://localhost:5400/search?table=\" + this.table).then(reponse => reponse.data).catch(function (error) {\n        console.log(\" error : \", error);\n      });\n      return result;\n    },\n\n    // Button déconnexion\n    deco() {\n      this.isConected = false;\n    },\n\n    // Button connexion\n    connect() {\n      this.isConected = true;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqBA,OAAOA,SAAP,MAAsB,uBAAtB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVD;AADU,GADC;;AAIbE,MAAI,GAAG;AACL,QAAIC,UAAS,GAAI,IAAjB;AAEA,WAAO;AACLA;AADK,KAAP;AAGD,GAVY;;AAWbC,SAAO,EAAE;AACP;AACAC,eAAW,EAAE,kBAAkB;AAC7B,YAAMC,MAAK,GAAI,MAAMC,KAAI,CACtBC,GADkB,CACd,wCAAwC,KAAKC,KAD/B,EAElBC,IAFkB,CAEZC,OAAD,IAAaA,OAAO,CAACT,IAFR,EAGlBU,KAHkB,CAGZ,UAAUC,KAAV,EAAiB;AACtBC,eAAO,CAACC,GAAR,CAAY,WAAZ,EAAyBF,KAAzB;AACD,OALkB,CAArB;AAMA,aAAOP,MAAP;AACD,KAVM;;AAYP;AACAU,QAAI,GAAG;AACL,WAAKb,UAAL,GAAkB,KAAlB;AACD,KAfM;;AAgBP;AACAc,WAAO,GAAG;AACR,WAAKd,UAAL,GAAkB,IAAlB;AACD;;AAnBM;AAXI,CAAf","names":["Connexion","components","data","isConected","methods","request_api","result","axios","get","table","then","reponse","catch","error","console","log","deco","connect"],"sourceRoot":"","sources":["D:\\www\\tristan.labaume\\Année 2021 - 2022\\M. Paul\\projet_vue\\src\\App.vue"],"sourcesContent":["<template>\n  <div v-if=\"isConected\" class=\"main\">\n    <div class=\"nav\">\n      <div class=\"logo\"></div>\n      <router-link to=\"/\"><div class=\"block\">Accueil</div></router-link>\n      <router-link to=\"/visites\"\n        ><div class=\"block\">Mes visites</div></router-link\n      >\n      <router-link to=\"/medecin\"><div class=\"block\">Médecin</div></router-link>\n      <router-link to=\"/medicament\"\n        ><div class=\"block\">Médicament</div></router-link\n      >\n      <button class=\"deco\" v-on:click=\"deco\">Déconnexion</button>\n    </div>\n    <router-view class=\"vue\"></router-view>\n  </div>\n\n  <Connexion v-else @connect=\"connect\" />\n</template>\n\n<script>\nimport Connexion from \"@/views/Connexion.vue\";\n\nexport default {\n  components: {\n    Connexion,\n  },\n  data() {\n    let isConected = true;\n\n    return {\n      isConected,\n    };\n  },\n  methods: {\n    // Faire des requettes a l'api\n    request_api: async function () {\n      const result = await axios\n        .get(\"http://localhost:5400/search?table=\" + this.table)\n        .then((reponse) => reponse.data)\n        .catch(function (error) {\n          console.log(\" error : \", error);\n        });\n      return result;\n    },\n\n    // Button déconnexion\n    deco() {\n      this.isConected = false;\n    },\n    // Button connexion\n    connect() {\n      this.isConected = true;\n    },\n  },\n};\n</script>\n\n<style>\n* {\n  margin: 0;\n  padding: 0;\n}\n.main {\n  width: 100%;\n  height: 100%;\n}\n.nav {\n  width: 100%;\n  height: 40px;\n  position: sticky;\n  display: flex;\n  border: 1px solid black;\n  align-items: center;\n  background: grey;\n}\n.block {\n  padding: 5px 20px;\n  margin: 0 2px;\n  border: 1px solid red;\n  cursor: pointer;\n}\n.vue {\n  width: 100%;\n  height: 100%;\n}\n.connexion {\n  width: 100%;\n  height: 100%;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}