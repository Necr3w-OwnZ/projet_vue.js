{"ast":null,"code":"import Connexion from \"@/views/Connexion.vue\";\nimport axios from \"axios\";\nimport lune from '@/assets/lunes.png';\nimport soleil from '@/assets/soleil.png';\nexport default {\n  components: {\n    Connexion\n  },\n\n  data() {\n    let isConnected = false;\n    let isLoading = true;\n    let imageLight = soleil;\n    return {\n      isConnected,\n      isLoading,\n      imageLight\n    };\n  },\n\n  methods: {\n    light() {\n      if (this.imageLight == soleil) {\n        this.imageLight == lune;\n      } else {\n        this.imageLight == soleil;\n      }\n    },\n\n    // Button déconnexion\n    deco() {\n      this.isConnected = false;\n    },\n\n    // Button connexion\n    connect() {\n      this.isConnected = true;\n      this.loading();\n    },\n\n    // Requette a l'api et page de chargement\n    async loading() {\n      this.isLoading = false;\n      let delay = null;\n      this.$store.dispatch(\"set_donnees\", await axios.get(\"http://172.29.1.39:5400/search\").then(reponse => reponse.data).then(delay = setInterval(() => {\n        this.isLoading = true;\n        clearInterval(delay);\n      }, 500)).catch(function (error) {\n        console.log(\" error : \", error);\n      }));\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAwBA,OAAOA,SAAP,MAAsB,uBAAtB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,IAAP,MAAiB,oBAAjB;AACA,OAAOC,MAAP,MAAmB,qBAAnB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVJ;AADU,GADC;;AAIbK,MAAI,GAAG;AACL,QAAIC,WAAU,GAAI,KAAlB;AACA,QAAIC,SAAQ,GAAI,IAAhB;AACA,QAAIC,UAAS,GAAIL,MAAjB;AAEA,WAAO;AACLG,iBADK;AAELC,eAFK;AAGLC;AAHK,KAAP;AAKD,GAdY;;AAebC,SAAO,EAAE;AACPC,SAAK,GAAE;AACL,UAAG,KAAKF,UAAL,IAAmBL,MAAtB,EAA6B;AAC3B,aAAKK,UAAL,IAAmBN,IAAnB;AACF,OAFA,MAGI;AACF,aAAKM,UAAL,IAAmBL,MAAnB;AACF;AACD,KARM;;AASP;AACAQ,QAAI,GAAG;AACL,WAAKL,WAAL,GAAmB,KAAnB;AACD,KAZM;;AAaP;AACAM,WAAO,GAAG;AACR,WAAKN,WAAL,GAAmB,IAAnB;AACA,WAAKO,OAAL;AACD,KAjBM;;AAkBP;AACA,UAAMA,OAAN,GAAgB;AACd,WAAKN,SAAL,GAAiB,KAAjB;AACA,UAAIO,KAAI,GAAI,IAAZ;AACA,WAAKC,MAAL,